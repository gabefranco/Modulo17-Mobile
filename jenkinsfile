pipeline {
    agent any

    stages {
        stage('Clonar o repositorio') {
            steps {
               git branch: 'main', url: 'https://github.com/gabefranco/Modulo17-Mobile.git'
            }
        }
     stage('Instalar dependencias') {
            steps {
               sh 'npm install' 
            }
        }
     stage('Executar testes') {
            steps {
               sh 'npm run wdio'
            }
        }
    }
}
